#include<iostream>
#include<string>
using namespace std;
int main() {
	cin.tie(NULL);                             //C의 stdio와 C++의 iostream의 동기화를 비활성화
	ios_base::sync_with_stdio(false);          //입출력 묶음을 풀어줌

	int n, a, b;
	cin >> n;
	for (int i = 0;i < n;i++) {
		cin >> a >> b;
		cout << a + b << "\n";                   //endl 대신 개행
	}
	return 0;
}
